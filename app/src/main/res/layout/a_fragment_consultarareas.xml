<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui_administrador.ConsultarAreas.ConsultarAreasF">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/logoshadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="363dp"
        android:layout_height="587dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="vertical">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView130"
                    android:layout_width="23dp"
                    android:layout_height="match_parent"
                    android:text=" " />

                <TextView
                    android:id="@+id/textView133"
                    android:layout_width="41dp"
                    android:layout_height="wrap_content"
                    android:text="Area"
                    android:textStyle="bold" />

                <Spinner
                    android:id="@+id/spinner15"
                    android:layout_width="224dp"
                    android:layout_height="27dp"
                    android:background="@drawable/spinlarge1" />

                <Button
                    android:id="@+id/button7"
                    android:layout_width="62dp"
                    android:layout_height="27dp"
                    android:background="@drawable/btnbuscarxx" />

                <TextView
                    android:id="@+id/textView134"
                    android:layout_width="20dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text=" " />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:orientation="vertical">

            <GridView
                android:layout_width="match_parent"
                android:layout_height="483dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView135"
                    android:layout_width="137dp"
                    android:layout_height="match_parent"
                    android:text=" " />

                <Button
                    android:id="@+id/button8"
                    android:layout_width="92dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/imprimir" />

                <TextView
                    android:id="@+id/textView136"
                    android:layout_width="25dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text=" " />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>